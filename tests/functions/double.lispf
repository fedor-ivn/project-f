(setq inf (divide 1.0 0.0))
(setq negInf (divide -1.0 0.0))
(setq nan (divide 0.0 0.0))

(func isNan (x) (nonequal x x))

(cond (not (equal +0.0 -0.0))
    (return false))
(cond (not (isNan nan))
    (return false))
(cond (not (isNan (minus inf inf)))
    (return false))
(cond (not (isNan (plus nan 4.0)))
    (return false))
(and (equal (plus negInf 1.0) negInf)
     (equal (minus 2.0 inf) negInf))
