(setq a (quote foo))
(setq b 'foo)
(setq c ''foo); `c` contains the list `(quote foo)`

(cond (not (isatom a))
    (return false))
(cond (not (isatom'a))
    (return false))
(cond (not (isatom b))
    (return false))
(cond (not (islist c))
    (return false))
(and (isatom (head c))
     (isatom (head (tail c))))
